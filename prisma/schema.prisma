// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Material {
  id Int

  materialName String
  type String
  pollutionLevel Float
  weight Float
  rewardPoints Int
  amount Float

  createdAt DateTime
  updatedAt DataTime
}

model Transporte {
  id            Int       
  dataSaida     DateTime  
  dataChegada   DateTime? 
  motorista     String    
  status        String

  createdAt DateTime 
  updatedAt DateTime 
}

