// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}



model Material {
  id Int

  materialName String
  type String
  pollutionLevel Float
  weight Float
  rewardPoints Int
  amount Float

  createdAt DateTime
  updatedAt DataTime
}