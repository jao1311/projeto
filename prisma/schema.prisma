// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default( autoincremente() )

  name String
  pass String
  email String
  cpf String
  phone String
 
  createdAt DataTime
  update Datatime
}


model Material {
  id Int @id @default( autoincremente() )

  materialName String
  type String
  pollutionLevel Float
  weight Float
  rewardPoints Int
  amount Float

  createdAt DateTime
  updatedAt DataTime
}

model Enterprise{
  id Int @id @default( autoincremente() ) 

  cnpj String
  adress String
  area String
  name String
  urlImage String
  isActive Boolean

  createdAt DateTime
  updatedAt DataTime

}

model Transporte {
  id            Int @id @default( autoincremente() )       
  dataSaida     DateTime  
  dataChegada   DateTime 
  motorista     String    
  status        String

  createdAt DateTime 
  updatedAt DateTime 
}

